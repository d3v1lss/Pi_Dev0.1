                        <a class="js-arrow" href="#">
                            <i class="fas fa-comment-alt"></i>Cinema
                            <span class="arrow">
                                    <i class="fas fa-angle-down"></i>
                                </span>
                        </a>
                        <ul class="list-unstyled navbar__sub-list js-sub-list">
                            <li>
                                <a href="{{  path('readfilm')}}">
                                    <i class="fas fa-table"></i>liste films</a>
                            </li>
                            <li>
                                <a href="{{  path('readsalle')}}">
                                    <i class="far fa-check-square"></i>liste salles</a>
                            </li>
                            <li>
                                <a href="{{  path('createfilm')}}">
                                    <i class="fas fa-table"></i>ajouter film</a>
                            </li>
                            <li>
                                <a href="{{  path('createsalle')}}">
                                    <i class="far fa-check-square"></i>ajouter salle</a>
                            </li>
                            <li>
                                <a href="{{  path('StatAvis')}}">
                                    <i class="fas fa-table"></i>Stat Avis</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fas fa-calendar-alt"></i> Evenement</a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fas fa-eye"></i> Bibliotheque</a>
                    </li>

                    <li class="has-sub">
                        <a class="js-arrow" href="#">
                            Connection
                            <span class="arrow">
                                    <i class="fas fa-angle-down"></i>
                                </span>
                        </a>
