                                <a href="{{ path('fos_user_security_logout') }} "
                                   class="fables-third-text-color fables-second-hover-color font-18 top-header-link px-3 px-md-2 px-lg-4 max-line-height">

                                    {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}</a>

                            {% else %}
                                <a href="{{ path('fos_user_security_login') }}"   class="fables-third-text-color fables-second-hover-color font-18 top-header-link px-3 px-md-2 px-lg-4 max-line-height">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a>
