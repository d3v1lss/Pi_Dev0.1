                <h4 class="name">{{ app.user.username }}</h4>


            </div>
            <nav class="navbar-sidebar2">
                <ul class="list-unstyled navbar__list">
                    <li>
                        <a href="{{  path('admin_homepage')}}">
                            <i class="fas fa-table"></i> Dashboard</a>
                    </li>



                    </li>
                    <li class="has-sub">
                        <a class="js-arrow" href="#">
                            <i class="fas fa-comment-alt"></i>Club
